package com.example.StockMicroservice;

public class Stock {
	int ID;
	String name;
	float price;
	
	public Stock() {
		super();
		
	}
	
	public Stock(int ID, String name,float price) {
		this.ID=ID;
		this.name=name;
		this.price=price;
	}
	
	public int getID() {
		return ID;
	}
	public void setID(int ID) {
		this.ID = ID;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public float getPrice() {
		return price;
	}
	public void setPrice(float price) {
		this.price = price;
	}
	

}
